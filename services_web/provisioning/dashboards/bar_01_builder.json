{
  "title": "Bar 01 - Builder",
  "uid": "bar-01-builder",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "",
  "tags": ["bars", "prometheus"],
  "editable": false,
  "panels": [
    {
      "type": "stat",
      "title": "Redis Connected",
      "gridPos": {"h": 7, "w": 6, "x": 0, "y": 0},
      "datasource": "Prometheus",
      "id": 1,
      "targets": [{"expr": "barbuilder_redis_connected", "refId": "A", "legendFormat": "{{service}}"}]
    },
    {
      "type": "stat",
      "title": "Errors (5m)",
      "gridPos": {"h": 7, "w": 6, "x": 0, "y": 7},
      "datasource": "Prometheus",
      "id": 4,
      "targets": [{"expr": "increase(barbuilder_errors_total[5m])", "refId": "A", "legendFormat": "{{service}}"}]
    },
    {
      "type": "timeseries",
      "title": "Ticks Read/sec",
      "gridPos": {"h": 7, "w": 12, "x": 6, "y": 0},
      "datasource": "Prometheus",
      "id": 2,
      "targets": [{"expr": "rate(barbuilder_read_total[1m])", "refId": "A", "legendFormat": "{{service}}"}]
    },
    {
      "type": "timeseries",
      "title": "Acks/sec",
      "gridPos": {"h": 7, "w": 12, "x": 6, "y": 7},
      "datasource": "Prometheus",
      "id": 3,
      "targets": [{"expr": "rate(barbuilder_acked_total[1m])", "refId": "A", "legendFormat": "{{service}}"}]
    },
    {
      "type": "timeseries",
      "title": "1m Bars/sec",
      "gridPos": {"h": 7, "w": 11, "x": 0, "y": 14},
      "datasource": "Prometheus",
      "id": 5,
      "targets": [{"expr": "rate(barbuilder_bar_1m_live_total[1m])", "refId": "A", "legendFormat": "1m Bars"}]
    },
    {
      "type": "timeseries",
      "title": "1D Bars/sec",
      "gridPos": {"h": 7, "w": 13, "x": 11, "y": 14},
      "datasource": "Prometheus",
      "id": 6,
      "targets": [{"expr": "rate(barbuilder_bar_1d_live_total[1m])", "refId": "A", "legendFormat": "1D Bars"}]
    },
    {
      "type": "timeseries",
      "title": "Process Duration p95",
      "gridPos": {"h": 7, "w": 12, "x": 0, "y": 21},
      "datasource": "Prometheus",
      "id": 7,
      "targets": [{"expr": "histogram_quantile(0.95, sum(rate(barbuilder_process_duration_seconds_bucket[5m])) by (le))", "refId": "A", "legendFormat": "p95 duration"}]
    }
