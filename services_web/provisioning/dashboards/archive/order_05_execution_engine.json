{
  "title": "Order 05 - Execution Engine",
  "uid": "order-05-execution-engine",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "tags": ["orders", "execution", "prometheus"],
  "editable": true,
  "panels": [
    {
      "type": "stat",
      "title": "Redis Connected",
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
      "datasource": "Prometheus",
      "id": 1,
      "targets": [{"expr": "execution_redis_connected", "refId": "A", "legendFormat": "Connected"}],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [{"color": "red", "value": null}, {"color": "green", "value": 1}]}
        }
      }
    },
    {
      "type": "stat",
      "title": "Ticks Read (5m)",
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
      "datasource": "Prometheus",
      "id": 2,
      "targets": [{"expr": "increase(execution_ticks_read_total[5m])", "refId": "A", "legendFormat": "Ticks"}]
    },
    {
      "type": "stat",
      "title": "Entry Hits Triggered (5m)",
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
      "datasource": "Prometheus",
      "id": 3,
      "targets": [{"expr": "increase(execution_entry_hits_total[5m])", "refId": "A", "legendFormat": "Entry Hits"}],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {"mode": "absolute", "steps": [{"color": "blue", "value": null}]}
        }
      }
    },
    {
      "type": "stat",
      "title": "Exit Hits Triggered (5m)",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
      "datasource": "Prometheus",
      "id": 4,
      "targets": [{"expr": "increase(execution_exit_requests_total[5m])", "refId": "A", "legendFormat": "Exit Hits"}]
    },
    {
      "type": "timeseries",
      "title": "Ticks Processed/sec",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
      "datasource": "Prometheus",
      "id": 5,
      "targets": [{"expr": "rate(execution_ticks_read_total[1m])", "refId": "A", "legendFormat": "Ticks/sec"}]
    },
    {
      "type": "timeseries",
      "title": "Entry Hits/sec",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
      "datasource": "Prometheus",
      "id": 6,
      "targets": [{"expr": "rate(execution_entry_hits_total[1m])", "refId": "A", "legendFormat": "Entry Hits/sec"}]
    },
    {
      "type": "timeseries",
      "title": "Exit Hits/sec",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
      "datasource": "Prometheus",
      "id": 7,
      "targets": [{"expr": "rate(execution_exit_requests_total[1m])", "refId": "A", "legendFormat": "Exit Hits/sec"}]
    },
    {
      "type": "timeseries",
      "title": "Tick Processing Latency p95 (seconds)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "datasource": "Prometheus",
      "id": 8,
      "targets": [{"expr": "histogram_quantile(0.95, sum(rate(execution_latency_seconds_bucket[5m])) by (le))", "refId": "A", "legendFormat": "p95 latency"}]
    },
    {
      "type": "timeseries",
      "title": "Latency Distribution (p50/p95/p99)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "datasource": "Prometheus",
      "id": 9,
      "targets": [
        {"expr": "histogram_quantile(0.50, sum(rate(execution_latency_seconds_bucket[5m])) by (le))", "refId": "A", "legendFormat": "p50"},
        {"expr": "histogram_quantile(0.95, sum(rate(execution_latency_seconds_bucket[5m])) by (le))", "refId": "B", "legendFormat": "p95"},
        {"expr": "histogram_quantile(0.99, sum(rate(execution_latency_seconds_bucket[5m])) by (le))", "refId": "C", "legendFormat": "p99"}
      ]
    }
  ]
}
