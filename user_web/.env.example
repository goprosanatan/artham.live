# Explicit API URL override (highest priority, optional).
VITE_API_ADDRESS=""

# Local development (`npm run dev`)
# If host is blank, browser hostname is used.
VITE_API_DEV_HOST=""
VITE_API_DEV_PORT="8000"

# Production build (`npm run build` / Docker image)
# Default is same-origin via nginx proxy in `user_web` container.
# Set host/port only when browser must call API directly.
# You can also pass these via root `.env` as Docker build args.
VITE_API_PROD_HOST=""
VITE_API_PROD_PORT=""
VITE_API_PROD_PROTOCOL=""
